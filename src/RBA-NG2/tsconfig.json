{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "commonjs",
    "experimentalDecorators": true,
    "skipLibCheck": true
  },
  "exclude": [
    "typings",
    "node_modules",
    "wwwroot/node_modules"
  ],
  "compileOnSave": true
}
